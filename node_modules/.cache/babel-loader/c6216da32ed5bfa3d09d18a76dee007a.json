A;AASA,UAAA,OAAA,CAAA,GAAA,CAAA,OAAA,EAAA,KAAA;;AACA,cAAA;AACA,YAAA,MAAA,CAAA,MAAA,CAAA,UAAA;;AACA,gBAAA,MAAA,CAAA,MAAA,IAAA,KAAA,EAAA;AACA,cAAA,MAAA,CAAA,MAAA,CAAA,eAAA,CAAA,WAAA,CAAA,WAAA,CAAA,KAAA;AACA,aAFA,MAEA,IAAA,MAAA,CAAA,MAAA,IAAA,SAAA,EAAA;AACA,cAAA,OAAA,CAAA,GAAA,CAAA,gBAAA,EAAA,IAAA,CAAA,SAAA,CAAA,KAAA,CAAA;AACA,cAAA,MAAA,CAAA,OAAA,CAAA,UAAA,CAAA,aAAA,EAAA,IAAA,CAAA,SAAA,CAAA,KAAA,CAAA;AACA;AACA,WARA,CAQA,OAAA,CAAA,EAAA;AACA,YAAA,MAAA,CAAA,MAAA,CAAA,UAAA,EADA,CAEA;;;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,eAAA;AACA;AACA,SAzBA,MAyBA;AACA,UAAA,MAAA,CAAA,MAAA,CAAA,UAAA;AACA;AACA,OAlCA,WAmCA,UAAA,GAAA,EAAA,CAAA,CAnCA;AAoCA;AA5LA,GA3BA;AAyNA,EAAA,UAAA,EAAA;AACA,cAAA,IADA;AAEA,eAAA,KAFA;AAGA,aAAA,GAHA;AAIA,iBAAA,OAJA;AAKA,kBAAA;AALA;AAzNA,CAAA","sourcesContent":["<template>\n\t<div class=\"app\">\n\t\t<div class=\"loadingding center\" v-show=\"!isLoading\"><van-loading size=\"30px\" color=\"#ff6666\" vertical>加载中</van-loading></div>\n\t\t<div class=\"content\" v-show=\"isLoading\">\n\t\t\t<div><p @click=\"loca()\">刷新</p></div>\n\t\t\t<div class=\"apply-content\" v-if=\"isApply\">\n\t\t\t\t<div class=\"header\">\n\t\t\t\t\t<div class=\"header-title\">\n\t\t\t\t\t\t<div class=\"header-img\">\n\t\t\t\t\t\t\t<!-- <img :src=\"babyInfo.head\" alt=\"\" v-if=\"babyInfo.head != ''\" /> -->\n\t\t\t\t\t\t\t<img v-if=\"babyInfo.sex == 1\" src=\"../../assets/image/my_avatar_boy@3x.png\" alt=\"\" />\n\t\t\t\t\t\t\t<img v-else src=\"../../assets/image/my_avatar_girl@3x.png\" alt=\"\" />\n\t\t\t\t\t\t</div>\n\t\t\t\t\t\t<div class=\"header-name\">\n\t\t\t\t\t\t\t<p>\n\t\t\t\t\t\t\t\t{{ babyInfo.nickName || babyInfo.nickname || '火火兔宝宝' }}\n\t\t\t\t\t\t\t\t<img v-if=\"babyInfo.sex == 1\" src=\"../../assets/image/my_navbar_boy@3x.png\" alt=\"\" />\n\t\t\t\t\t\t\t\t<img v-else=\"babyInfo.sex == 2\" src=\"../../assets/image/my_navbar_girl@3x.png\" alt=\"\" />\n\t\t\t\t\t\t\t\t<img v-if=\"memberInfo.code == 1\" src=\"../../assets/image/my_vip_icon@3x.png\" alt=\"\" />\n\t\t\t\t\t\t\t</p>\n\t\t\t\t\t\t\t<p>{{ getBirthSlot(babyInfo.birthday) }}</p>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</div>\n\t\t\t\t\t<div class=\"header-ranking\" @click=\"setRouter('ranking')\">宝宝排名</div>\n\t\t\t\t</div>\n\t\t\t\t<v-data></v-data>\n\t\t\t\t<div class=\"sign-day\">\n\t\t\t\t\t<div class=\"day\">\n\t\t\t\t\t\t<p>\n\t\t\t\t\t\t\t智慧早教第\n\t\t\t\t\t\t\t<span>{{ applyTime || 0 }}</span>\n\t\t\t\t\t\t\t天计划\n\t\t\t\t\t\t</p>\n\t\t\t\t\t\t<p>目标30min</p>\n\t\t\t\t\t</div>\n\t\t\t\t\t<p class=\"day-sum\">累计坚持{{ sumDay }}天</p>\n\t\t\t\t</div>\n\t\t\t\t<p @click=\"goReport\">报告</p>\n\t\t\t\t<div class=\"music-btn\" @click=\"education()\"><p>今日早教课程</p></div>\n\t\t\t\t<div class=\"course-key\">\n\t\t\t\t\t<v-title :title=\"title[0]\"></v-title>\n\t\t\t\t\t<div class=\"key-list\">\n\t\t\t\t\t\t<ul v-if=\"keyArray.length != 0\">\n\t\t\t\t\t\t\t<li v-for=\"item in keyArray\">\n\t\t\t\t\t\t\t\t<span>{{ item }}</span>\n\t\t\t\t\t\t\t</li>\n\t\t\t\t\t\t</ul>\n\t\t\t\t\t\t<div v-else class=\"content-null\"><p>暂无关键词</p></div>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t\t<div class=\"course-emphasis\">\n\t\t\t\t\t<v-title :title=\"title[1]\"></v-title>\n\t\t\t\t\t<div class=\"key-list\" v-if=\"isPie\"><v-pie :pieData=\"keyList\"></v-pie></div>\n\t\t\t\t\t<div v-else class=\"content-null\"><p>暂无强化重点</p></div>\n\t\t\t\t</div>\n\t\t\t\t<div class=\"course-period\">\n\t\t\t\t\t<v-title :title=\"title[2]\" :age=\"countAge(babyYear, babyMonth)\"></v-title>\n\t\t\t\t\t<div class=\"period-list\" v-if=\"periodList.length != 0\">\n\t\t\t\t\t\t<ul>\n\t\t\t\t\t\t\t<li v-for=\"(item, index) in periodList\" :key=\"index\">\n\t\t\t\t\t\t\t\t<div class=\"period-img\"><img :src=\"item.level1_logo\" alt=\"\" /></div>\n\t\t\t\t\t\t\t\t<div class=\"period-name\">\n\t\t\t\t\t\t\t\t\t<p class=\"van-ellipsis\">{{ item.level1 }}</p>\n\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t\t<div class=\"period-label\">\n\t\t\t\t\t\t\t\t\t<p class=\"van-multi-ellipsis--l3\">{{ item.level2 }}</p>\n\t\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t</li>\n\t\t\t\t\t\t</ul>\n\t\t\t\t\t</div>\n\t\t\t\t\t<div v-else class=\"content-null\"><p>暂无关键期</p></div>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t\t<div class=\"noapply-content\" v-else>\n\t\t\t\t<div class=\"apply-img\"><img src=\"../../assets/image/apply.jpg\" alt=\"\" /></div>\n\t\t\t\t<div class=\"apply-btn\" @click=\"userApply\"><p>立刻报名</p></div>\n\t\t\t</div>\n\t\t\t<v-suspend></v-suspend>\n\t\t\t<v-babaList></v-babaList>\n\t\t</div>\n\t</div>\n</template>\n\n<script>\nimport Title from '@/components/Title.vue';\nimport Data from '@/components/Data.vue';\nimport Pie from '@/components/Pie.vue';\nimport Suspend from '@/components/Suspend.vue';\nimport BabaList from '@/components/BabyList.vue';\nimport { mapActions, mapMutations, mapState, mapGetters } from 'vuex';\nexport default {\n\tdata() {\n\t\treturn {\n\t\t\tisLoading: false,\n\t\t\ttitle: ['今日学习关键词', '今日强化重点', '宝宝关键期'],\n\t\t\tage: '0个月',\n\t\t\tperiodList: [],\n\t\t\tcurrentTime: null,\n\t\t\tcurrentMouth: null,\n\t\t\tsumDay: 0,\n\t\t\tkeyList: [],\n\t\t\tkeyArray: [],\n\t\t\tisPie: false,\n\t\t\tapplyTime: 0,\n\t\t\tbabyYear: 0,\n\t\t\tbabyMonth: 0,\n\t\t\tisApply: false\n\t\t};\n\t},\n\tcomputed: {\n\t\t...mapState(['system', 'babyInfo', 'memberInfo'])\n\t},\n\tcreated() {\n\t\tthis.current();\n\t\tthis.getSumTime();\n\t\tconsole.log('babyInfo', this.babyInfo);\n\t},\n\tmethods: {\n\t\tgoReport() {\n\t\t\tthis.$router.push({ name: 'report' });\n\t\t},\n\t\tloca() {\n\t\t\tlocation.reload();\n\t\t},\n\t\tsetRouter(val) {\n\t\t\tthis.$router.push({ name: val });\n\t\t},\n\t\tcountAge(year, month) {\n\t\t\tvar sum = year * 12 + month;\n\t\t\tconsole.log('nianling,', sum);\n\t\t\tif (sum < 13) {\n\t\t\t\treturn month + '-' + (month + 1) + '个月';\n\t\t\t} else if (sum >= 13 && sum < 18) {\n\t\t\t\treturn '1-1.5岁';\n\t\t\t} else if (sum >= 18 && sum < 24) {\n\t\t\t\treturn '1.5-2岁';\n\t\t\t} else if (sum >= 24 && sum < 36) {\n\t\t\t\treturn '2-3岁';\n\t\t\t} else if (sum >= 36 && sum < 48) {\n\t\t\t\treturn '3-4岁';\n\t\t\t} else if (sum >= 48 && sum < 60) {\n\t\t\t\treturn '4-5岁';\n\t\t\t} else if (sum >= 60 && sum < 72) {\n\t\t\t\treturn '5-6岁';\n\t\t\t}\n\t\t},\n\t\t//计算宝宝多少天\n\t\tgetBirthSlot(date) {\n\t\t\tvar time;\n\t\t\ttry {\n\t\t\t\ttime = date\n\t\t\t\t\t.replace('年', '-')\n\t\t\t\t\t.replace('月', '-')\n\t\t\t\t\t.replace('日', '');\n\t\t\t} catch (e) {\n\t\t\t\t//TODO handle the exception\n\t\t\t\ttime = this.currentTime;\n\t\t\t}\n\t\t\tlet birthDay = new Date(time);\n\t\t\tlet nowDate = new Date();\n\t\t\tlet date1 = Date.parse(birthDay);\n\t\t\tlet date2 = Date.parse(nowDate);\n\t\t\tlet day = Math.floor((date2 - date1) / (60 * 60 * 1000 * 24));\n\t\t\tlet age = '';\n\t\t\tlet year = Math.floor(day / 365);\n\t\t\tlet y = day % 365;\n\t\t\tlet month = Math.floor(y / 30);\n\t\t\tlet d = Math.floor((day % 365) % 30);\n\t\t\tage += year + '岁' + month + '月' + d + '天';\n\t\t\tthis.babyYear = year;\n\t\t\tthis.babyMonth = month;\n\t\t\treturn age;\n\t\t},\n\t\tgetUserDayCourse() {\n\t\t\tlet self = this;\n\t\t\tthis.$axios\n\t\t\t\t.getDayKey(this.currentTime)\n\t\t\t\t.then(res => {\n\t\t\t\t\tlet pieData = res.data.data.emphasis;\n\t\t\t\t\tpieData.forEach(function(Data, index) {\n\t\t\t\t\t\tlet v = Data.rate.replace('%', '');\n\t\t\t\t\t\tlet obj = {\n\t\t\t\t\t\t\tname: Data.ability + Data.rate,\n\t\t\t\t\t\t\tvalue: v,\n\t\t\t\t\t\t\tsize: v\n\t\t\t\t\t\t};\n\t\t\t\t\t\tself.keyList.push(obj);\n\t\t\t\t\t});\n\t\t\t\t\tconsole.log('self.keyList', self.keyList);\n\t\t\t\t\tlet arr = res.data.data.key;\n\t\t\t\t\tself.keyArray = arr.split('、');\n\t\t\t\t\tself.isPie = true;\n\t\t\t\t})\n\t\t\t\t.catch(err => {});\n\t\t\tthis.$axios\n\t\t\t\t.getBabyPeriod(this.currentTime)\n\t\t\t\t.then(res => {\n\t\t\t\t\tthis.periodList = res.data.data.list;\n\t\t\t\t})\n\t\t\t\t.catch(err => {});\n\t\t\tthis.$axios\n\t\t\t\t.sumTime(this.currentMouth)\n\t\t\t\t.then(res => {\n\t\t\t\t\tthis.sumDay = res.data.data.clockDays;\n\t\t\t\t})\n\t\t\t\t.catch(err => {});\n\t\t},\n\t\tgetSumTime() {\n\t\t\tthis.$axios\n\t\t\t\t.userApplyTime()\n\t\t\t\t.then(res => {\n\t\t\t\t\tthis.applyTime = this.computedTime(res.data.data);\n\t\t\t\t\tthis.isLoading = true;\n\t\t\t\t\tlet time = res.data.data;\n\t\t\t\t\tif (time.indexOf('0') != -1) {\n\t\t\t\t\t\tthis.getUserDayCourse();\n\t\t\t\t\t\tthis.isApply = true;\n\t\t\t\t\t}\n\t\t\t\t})\n\t\t\t\t.catch(err => {});\n\t\t},\n\t\tuserApply() {\n\t\t\tif (this.babyInfo.length == 0) {\n\t\t\t\tthis.$toast('请先在APP中添加宝宝信息');\n\t\t\t\treturn;\n\t\t\t}\n\t\t\tif (this.memberInfo.code == 0) {\n\t\t\t\tthis.$router.push({ name: 'index' });\n\t\t\t\tthis.$toast('请先开通会员');\n\t\t\t\treturn;\n\t\t\t}\n\t\t\tthis.$toast.loading({\n\t\t\t\tmessage: '报名中...',\n\t\t\t\tforbidClick: true\n\t\t\t});\n\t\t\tthis.$axios\n\t\t\t\t.userApply()\n\t\t\t\t.then(res => {\n\t\t\t\t\tif (res.data.code == 1) {\n\t\t\t\t\t\tthis.$toast('报名成功');\n\t\t\t\t\t\tthis.getUserDayCourse();\n\t\t\t\t\t\tthis.isApply = true;\n\t\t\t\t\t\treturn true;\n\t\t\t\t\t}\n\t\t\t\t})\n\t\t\t\t.catch(err => {});\n\t\t},\n\t\tcomputedTime(old) {\n\t\t\t//传入之前的时间  时间格式为(YY-MM-DD HH:MM:SS)\n\t\t\tconsole.log(old);\n\t\t\tvar returnText = '';\n\t\t\tvar nowDate = new Date().getTime(); //当前时间\n\t\t\tvar setDate = new Date(old).getTime();\n\t\t\tconsole.log('setDate', setDate);\n\t\t\tvar times = Math.floor((nowDate - setDate) / 1000);\n\t\t\tconsole.log('times', times / (60 * 60 * 24));\n\t\t\treturnText = Math.ceil(times / (60 * 60 * 24));\n\t\t\tconsole.log('returnText11====', returnText);\n\t\t\treturn returnText;\n\t\t},\n\t\tcurrent() {\n\t\t\tvar dd = new Date();\n\t\t\tvar y = dd.getFullYear();\n\t\t\tvar m = dd.getMonth() + 1; //获取当前月份的日期\n\t\t\tvar d = dd.getDate();\n\t\t\tthis.currentTime = y + '-' + m + '-' + d;\n\t\t\tthis.currentMouth = m;\n\t\t},\n\t\teducation() {\n\t\t\tthis.$axios\n\t\t\t\t.getDayCourse(this.currentTime)\n\t\t\t\t.then(res => {\n\t\t\t\t\tconsole.log('点击====');\n\t\t\t\t\t// let array = { title: '智慧早教第' + this.applyTime + '天', course: [] };\n\t\t\t\t\tlet array = [];\n\t\t\t\t\tif (res.data.code == 1) {\n\t\t\t\t\t\tlet item = res.data.data;\n\t\t\t\t\t\titem.forEach(function(data, index) {\n\t\t\t\t\t\t\tlet obj = {\n\t\t\t\t\t\t\t\turl: data.url,\n\t\t\t\t\t\t\t\tid: data.id,\n\t\t\t\t\t\t\t\tname: data.name\n\t\t\t\t\t\t\t};\n\t\t\t\t\t\t\t// array.course.push(obj);\n\t\t\t\t\t\t\tarray.push(obj);\n\t\t\t\t\t\t});\n\t\t\t\t\t\tconsole.log('array', array);\n\t\t\t\t\t\ttry {\n\t\t\t\t\t\t\tthis.$toast('获取早教课程成功');\n\t\t\t\t\t\t\tif (this.system == 'ios') {\n\t\t\t\t\t\t\t\twindow.webkit.messageHandlers.course_play.postMessage(array);\n\t\t\t\t\t\t\t} else if (this.system == 'android') {\n\t\t\t\t\t\t\t\tconsole.log('安卓============', JSON.stringify(array));\n\t\t\t\t\t\t\t\twindow.android.playCourse('course_play', JSON.stringify(array));\n\t\t\t\t\t\t\t}\n\t\t\t\t\t\t} catch (e) {\n\t\t\t\t\t\t\tthis.$toast('获取早教课程失败');\n\t\t\t\t\t\t\t//TODO handle the exception\n\t\t\t\t\t\t\tconsole.log('请在app中打开11111');\n\t\t\t\t\t\t}\n\t\t\t\t\t} else {\n\t\t\t\t\t\tthis.$toast('暂无今日早教课程');\n\t\t\t\t\t}\n\t\t\t\t})\n\t\t\t\t.catch(err => {});\n\t\t}\n\t},\n\tcomponents: {\n\t\t'v-data': Data,\n\t\t'v-title': Title,\n\t\t'v-pie': Pie,\n\t\t'v-suspend': Suspend,\n\t\t'v-babaList': BabaList\n\t}\n};\n</script>\n\n<style lang=\"less\" scoped>\n@import '../../assets/css/home.less';\n.apply-img {\n\twidth: 100%;\n\theight: 100%;\n\timg {\n\t\twidth: 100%;\n\t\theight: 100%;\n\t}\n}\n.apply-btn {\n\twidth: 100%;\n\ttext-align: center;\n\tposition: fixed;\n\tbottom: 5%;\n\tleft: 0;\n\ttext-align: center;\n\tp {\n\t\twidth: 311px;\n\t\theight: 48px;\n\t\tline-height: 48px;\n\t\tbackground-color: #ff9f80;\n\t\tborder-radius: 24px;\n\t\tfont-size: 17px;\n\t\tcolor: #fff;\n\t\tmargin: 0 auto;\n\t}\n}\n</style>\n"],"sourceRoot":"src/views/Home"}]}