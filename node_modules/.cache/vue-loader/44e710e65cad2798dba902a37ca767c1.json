\tlet v = Data.rate.replace('%', '');\n\t\t\t\t\t\tlet obj = {\n\t\t\t\t\t\t\tname: Data.ability + Data.rate,\n\t\t\t\t\t\t\tvalue: v,\n\t\t\t\t\t\t\tsize: v\n\t\t\t\t\t\t};\n\t\t\t\t\t\tself.keyList.push(obj);\n\t\t\t\t\t});\n\t\t\t\t\tconsole.log('self.keyList', self.keyList);\n\t\t\t\t\tlet arr = res.data.data.key;\n\t\t\t\t\tself.keyArray = arr.split('、');\n\t\t\t\t\tself.isPie = true;\n\t\t\t\t})\n\t\t\t\t.catch(err => {});\n\t\t\tthis.$axios\n\t\t\t\t.getBabyPeriod(this.currentTime)\n\t\t\t\t.then(res => {\n\t\t\t\t\tthis.periodList = res.data.data.list;\n\t\t\t\t})\n\t\t\t\t.catch(err => {});\n\t\t\tself.isLoading = true;\n\t\t},\n\t\tgetSumTime() {\n\t\t\tthis.$axios\n\t\t\t\t.sumTime(this.currentMouth)\n\t\t\t\t.then(res => {\n\t\t\t\t\tthis.sumDay = res.data.data.clockDays;\n\t\t\t\t})\n\t\t\t\t.catch(err => {});\n\t\t\tthis.$axios\n\t\t\t\t.userApplyTime()\n\t\t\t\t.then(res => {\n\t\t\t\t\tthis.applyTime = this.computedTime(res.data.info);\n\t\t\t\t})\n\t\t\t\t.catch(err => {});\n\t\t},\n\t\tcomputedTime(old) {\n\t\t\t//传入之前的时间  时间格式为(YY-MM-DD HH:MM:SS)\n\t\t\tvar returnText = '';\n\t\t\tvar nowDate = new Date().getTime(); //当前时间\n\t\t\tvar setDate = new Date(old).getTime();\n\t\t\tvar times = Math.floor((nowDate - setDate) / 1000);\n\t\t\tif (times > 60 * 60 * 24) {\n\t\t\t\treturnText = Math.floor(times / (60 * 60 * 24));\n\t\t\t}\n\t\t\tconsole.log('returnText====', returnText);\n\t\t\treturn returnText;\n\t\t},\n\t\tcurrent() {\n\t\t\tvar dd = new Date();\n\t\t\tvar y = dd.getFullYear();\n\t\t\tvar m = dd.getMonth() + 1; //获取当前月份的日期\n\t\t\tvar d = dd.getDate();\n\t\t\tthis.currentTime = y + '-' + m + '-' + d;\n\t\t\tthis.currentMouth = m;\n\t\t},\n\t\teducation() {\n\t\t\tthis.$axios\n\t\t\t\t.getDayCourse()\n\t\t\t\t.then(res => {\n\t\t\t\t\tlet array = [];\n\t\t\t\t\tif (res.data.code == 1) {\n\t\t\t\t\t\tlet item = res.data.data;\n\t\t\t\t\t\titem.forEach(function(data, index) {\n\t\t\t\t\t\t\tlet obj = {\n\t\t\t\t\t\t\t\turl: data.url,\n\t\t\t\t\t\t\t\tid: data.id,\n\t\t\t\t\t\t\t\tname: data.name\n\t\t\t\t\t\t\t};\n\t\t\t\t\t\t\tarray.push(obj);\n\t\t\t\t\t\t});\n\t\t\t\t\t\ttry {\n\t\t\t\t\t\t\tif (this.system == 'ios') {\n\t\t\t\t\t\t\t\twindow.webkit.messageHandlers.course_play.postMessage(array);\n\t\t\t\t\t\t\t} else if (this.system == 'android') {\n\t\t\t\t\t\t\t\tconsole.log('安卓============');\n\t\t\t\t\t\t\t\twindow.android.playCourse('course_play', JSON.stringify(array));\n\t\t\t\t\t\t\t}\n\t\t\t\t\t\t} catch (e) {\n\t\t\t\t\t\t\t//TODO handle the exception\n\t\t\t\t\t\t\tconsole.log('请在app中打开11111');\n\t\t\t\t\t\t}\n\t\t\t\t\t}\n\t\t\t\t})\n\t\t\t\t.catch(err => {});\n\t\t\t// let data = [\n\t\t\t// \t{ url: 'https://resource.alilo.com.cn/res/4,01996ff43bb507.mp3', id: 1, name: '火火兔' },\n\t\t\t// \t{ url: 'https://resource.alilo.com.cn/res/2,019977d3f0fbf8.mp3', id: 1, name: '火火兔' },\n\t\t\t// \t{ url: 'https://resource.alilo.com.cn/res/7,019976e5c1fbd9.mp3', id: 1, name: '火火兔' }\n\t\t\t// ];\n\t\t}\n\t},\n\tcomponents: {\n\t\t'v-data': Data,\n\t\t'v-title': Title,\n\t\t'v-pie': Pie\n\t}\n};\n</script>\n\n<style lang=\"less\" scoped>\n@import '../../assets/css/home.less';\n</style>\n"]}]}